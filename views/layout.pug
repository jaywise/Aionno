doctype html
html
  head
    title= Aionno
    meta(charset='utf-8')
    link(rel='stylesheet', type='text/css', href='../static/css/bootstrap.min.css')
    link(rel='stylesheet', type='text/css', href='../static/css/style.css')
  body
    .container
      h1.title Aionno
      h3.sub-title Find better classroom strategies
      nav.row.navbar.navbar-expand-lg.navbar-light.bg-light
