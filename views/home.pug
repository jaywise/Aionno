extends layout

block content
  .container
    .row
      .sm-col-4.top-dd
        h5 Student is
        select(id='studentState' name='studentState')
          option(value='Any') any
          option(value='anxious') anxious
          option(value='aggressive') aggressive
          option(value='timid') timid
          option(value='tired') tired
          option(value='hyperactive') hyperactive
      .sm-col-4.top-dd
        h5 Situation is
        select(id='situationState' name='situationState')
          option(value='Any') any
          option(value='lecture') lecture
          option(value='group-work') group work
          option(value='independent-work') independent work
          option(value='transition') transition
          option(value='student-presentation') student presentation
    .container.strategy-blocks
      #listOfObjects
